msgid "plugins.generic.credit.name"
msgstr "CRediT Plugin"

msgid "plugins.generic.credit.description"
msgstr "This plugin adds support for the NISO CRediT contributor vocabulary."

msgid "plugins.generic.credit.contributorRoles"
msgstr "Contributor Roles"

msgid "plugins.generic.credit.contributorRoles.description"
msgstr "Please select the contributor's roles according to the <a href=\"https://credit.niso.org/\" target="_new">NISO CRediT list</a>."

msgid "plugins.generic.credit.settings.description"
msgstr "The CRediT vocabulary can interact with OJS in various ways. Use this form to choose which integrations should be used."

msgid "plugins.generic.credit.showCreditRoles"
msgstr "Show CRediT roles by author names on article landing pages."

msgid "plugins.generic.credit.invalidCreditRole"
msgstr "You have entered an invalid CRediT role. Please double-check the list of CRediT roles."
